#include <stdio.h>
int CalPercentage(int science,int maths,int hindi);

int main (){
    int science =85;
    int maths=90;
    int hindi =100;
    printf("The percentage is:%d%%",CalPercentage(science,maths,hindi));
    return 0;
}

int CalPercentage(int science,int maths,int hindi){
    return ((science+maths+hindi)/3);
}
